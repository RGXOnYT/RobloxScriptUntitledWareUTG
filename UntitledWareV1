makeBtn("Anti-Fling", true, "AntiFling", "Prevents a bug from flinging yo ahh (RECOMMENDED TO KEEP ON)")

makeBtn("Discord Server", false, nil, "Join the community!", function(b) setclipboard("https://discord.gg/VTZGSXZ9Vd"); b.Text = "Copied!"; task.wait(2); b.Text = "Discord Server" end, Color3.fromRGB(88, 101, 242))
makeBtn("Youtube Channel", false, nil, "Sub to the king!", function(b) setclipboard("https://www.youtube.com/@RGX-275"); b.Text = "Copied!"; task.wait(2); b.Text = "Youtube Channel" end, Color3.fromRGB(255, 0, 0))
makeBtn("Unload", false, nil, "Kill script", function() CONFIG.Running = false; ScreenGui:Destroy(); if Humanoid then Humanoid.WalkSpeed = 16 end end, Color3.fromRGB(30, 30, 30))

updateFly()
